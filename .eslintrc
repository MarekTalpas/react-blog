{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "import/no-named-as-default": 0,
    "no-shadow": 0,
    "global-require": 0,
    "arrow-body-style": 0
  }
}